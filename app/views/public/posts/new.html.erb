<h2>あなたのおすすめのスキンケア商品を投稿しましょう！！</h2>
  <%= form_with model: @items do |f| %>

  <h4>商品画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <h4>商品名</h4>
  <%= f.text_field :name %><!--ここら辺はおかしいかもしれない-->

  <h4>価格</h4>
  <%= f.text_field :value %><!--ここら辺はおかしいかもしれない-->

  <h4>カテゴリー名</h4>
  <!--div class="dropdown"--><!--%= f.select :'category[name]', *[1..10],include_blank: "カテゴリー名を選択してください" %--><!--/div-->

  <h4>感想</h4>
  <%= f.text_field :review %>

  <!--おすすめ度、余裕があれば入れる-->
  <%= f.submit '投稿' %>
<% end %>

